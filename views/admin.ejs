      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th scope="col">Approved/Delete</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i =0;i< user_list.length;i++){%>
    <tr>
      <th scope="row"><%=user_list[i]._id%></th>
      <td><%=user_list[i].firstname%></td>
      <td><%=user_list[i].lastname%></td>
      <td><%=user_list[i].email%></td>
      <td><a href="/approve/<%= user_list[i]._id %>"><i class="fas fa-check-circle fa-3x"></i> </a>  <a href="/delete/<%=user_list[i]._id%>"><i class="far fa-times-circle fa-3x"></i></a></td>
    </tr><%}%>
  </tbody>
</table>
<br>
<br>
<hr style="color:white; width:100%">
<br><br>
<h1 style="color:white">Approved Users</h1>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col">Email</th>
      <th scope="col">Remove Access</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i =0;i< approved_user.length;i++){%>
    <tr>
      <th scope="row"><%=approved_user[i]._id%></th>
      <td><%=approved_user[i].firstname%></td>
      <td><%=approved_user[i].lastname%></td>
      <td><%=approved_user[i].email%></td>
      <td><a href="/reject/<%=approved_user[i]._id%>"><i class="far fa-times-circle fa-3x"></i></a></td>
    </tr><%}%>
  </tbody>
</table>



